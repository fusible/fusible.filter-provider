{
    "name": "fusible/filter-provider",
    "description": "Aura\\Filter Provider for Aura\\Di",
    "homepage":"https://github.com/fusible/fusible.filter-provider",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "require": {
        "aura/di": "^4.0",
        "aura/filter": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Fusible\\FilterProvider\\": "src/"
        }
    },
    "autoload-dev": {
